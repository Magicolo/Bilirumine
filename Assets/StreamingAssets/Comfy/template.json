{
    "prompt": {
        "4": {
            "inputs": {
                "ckpt_name": "$model"
            },
            "class_type": "CheckpointLoaderSimple"
        },
        "5": {
            "inputs": {
                "width": $width,
                "height": $height,
                "batch_size": 1
            },
            "class_type": "EmptyLatentImage"
        },
        "6": {
            "inputs": {
                "text": "($positive) $prompt",
                "clip": [
                    "4",
                    1
                ]
            },
            "class_type": "CLIPTextEncode"
        },
        "7": {
            "inputs": {
                "text": "($negative) $forbid",
                "clip": [
                    "4",
                    1
                ]
            },
            "class_type": "CLIPTextEncode"
        },
        "8": {
            "inputs": {
                "samples": [
                    "10",
                    0
                ],
                "vae": [
                    "4",
                    2
                ]
            },
            "class_type": "VAEDecode"
        },
        "10": {
            "inputs": {
                "add_noise": true,
                "noise_seed": $seed,
                "cfg": $guidance,
                "model": [
                    "4",
                    0
                ],
                "positive": [
                    "6",
                    0
                ],
                "negative": [
                    "7",
                    0
                ],
                "sampler": [
                    "12",
                    0
                ],
                "sigmas": [
                    "42",
                    0
                ],
                "latent_image": [
                    "5",
                    0
                ]
            },
            "class_type": "SamplerCustom"
        },
        "12": {
            "inputs": {
                "sampler_name": "$sampler"
            },
            "class_type": "KSamplerSelect"
        },
        "36": {
            "inputs": {
                "filename_prefix": "$prefix",
                "images": [
                    "8",
                    0
                ]
            },
            "class_type": "SaveImage"
        },
        "42": {
            "inputs": {
                "scheduler": "$scheduler",
                "denoise": 1,
                "steps": $steps,
                "model": [
                    "4",
                    0
                ]
            },
            "class_type": "BasicScheduler"
        }
    }
}